<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.bfrc.dataaccess.model.contact.FeedbackType" schema="RTMS_WEBDB" table="BFRC_WEB_SITE_FEEDBACK_TYPE">
    <id name="feedbackTypeId" type="int">
      <column name="FEEDBACK_TYPE_ID" precision="5" scale="0"/>
      <generator class="assigned"/>
    </id>
    <property name="name" type="string">
      <column length="50" name="FEEDBACK_TYPE_NAME"/>
    </property>
  </class>
</hibernate-mapping>
